<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/qubitscourse/db" VERSION="20230213" COMMENT="XMLDB file for Moodle local/qubitscourse"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_qubits_course" COMMENT="Qubits Course - Tenants and course details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="site_id" TYPE="int" LENGTH="12" NOTNULL="true" SEQUENCE="false" COMMENT="Site Id"/>
        <FIELD NAME="course_id" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="15" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="15" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
